    "description": "A CLI tool for managing translation files and syncing entries with view files. Allows translations to be synced with views, get/set values, and list empty values.",
    "keywords": ["translations", "multi-language", "multilanguage", "languages"],
    "homepage": "http://github.com/martynbiz/cattlog",
    "license": "MIT",
    "authors": [
        {
            "name": "Martyn Bissett",
            "email": "martynbissett@yahoo.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
    		"MartynBiz\\Translate\\": "src/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "bin": [
        "bin/cattlog"
    ]
}
